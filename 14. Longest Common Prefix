const longestCommonPrefix = (strs) => {
    let answer = ""
    for(i =1; i<1000; i++){
        let arr = strs.filter(x => strs[0].slice(0, i) === x.slice(0, i))
          if(arr.length !== strs.length){
              return answer
          }
        
            answer = strs[0].slice(0, i)
        }
        return strs[0]
    }
    
