var fizzBuzz = function(n) {
    let arr = []
    for(i=0; i<n; i++){
        let word = ""
        if((i+1)%3 === 0){
            word = "Fizz"
        }
        if((i+1)%5 === 0){
            word = `${word}Buzz`
        }
        if(word===""){
            word = `${i+1}`
        }
        arr.push(word)
    }
    return arr
};
